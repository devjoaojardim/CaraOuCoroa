<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jvjp.caraoucoroa">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_cara"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_cara_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CaraOuCoroa">
        <activity android:name=".activity.ResultActivity"></activity>
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>